<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_verifications">

    <fragment
        android:id="@+id/navigation_verifications"
        android:name="com.example.serviceapp.ui.verifications.VerificationsFragment"
        android:label="@string/scoreboard"
        tools:layout="@layout/fragment_verifications" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.serviceapp.ui.dashboard.DashboardFragment"
        android:label="@string/livefeed"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.serviceapp.ui.home.HomeFragment"
        android:label="@string/profile"
        tools:layout="@layout/fragment_home" />

</navigation>